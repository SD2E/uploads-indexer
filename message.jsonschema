{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "title": "AbacoAgavePathMessage",
    "description": "Contains an agave-canonical path and optional routing information",
    "type": "object",
    "properties": {
        "uri": {
            "type": "string",
            "format": "uri",
            "description": "'uri' is an object-store equivalent to a file path."
        },
        "routings" : {}
    },
    "required": ["uri"]
}
